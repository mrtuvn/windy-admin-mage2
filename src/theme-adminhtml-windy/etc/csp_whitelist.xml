<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="font-src">
            <values>
                <value id="google-base" type="host">https://fonts.gstatic.com</value>
                <value id="googleapis" type="host">https://fonts.googleapis.com</value>
            </values>
        </policy>
        <policy id="script-src">
            <values>
                <value id="unpkg" type="host">https://unpkg.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
